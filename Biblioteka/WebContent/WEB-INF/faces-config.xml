<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
<application>
  <resource-bundle>
   <base-name>resources.textMain</base-name>
   <var>txtMain</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>resources.textError</base-name>
   <var>txtError</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>resources.textDesc</base-name>
   <var>txtDesc</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>resources.textBook</base-name>
   <var>txtBook</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>resources.textUser</base-name>
   <var>txtUser</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>resources.textBorrow</base-name>
   <var>txtBorrow</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>resources.textRules</base-name>
   <var>txtRules</var>
  </resource-bundle>
  <locale-config>
   <default-locale>pl_PL</default-locale>
  </locale-config>
 </application>
</faces-config>
